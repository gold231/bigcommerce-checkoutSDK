{
  "core/account/_private/navigation": "<nav class=\"account-nav\">\n  <ul class=\"account-nav-list\">\n    <li\n      class=\"\n        account-nav-item\n        {{#if nav_page '===' 'orders'}}is-active{{/if}}\n      \"\n    >\n      <a class=\"account-nav-link\" href=\"{{urls.account.orders.all}}\">\n        {{lang 'core.account.menu.orders'}}\n      </a>\n    </li>\n\n    {{#if settings.returns_enabled}}\n      <li\n        class=\"\n          account-nav-item\n          {{#if nav_page '===' 'returns'}}is-active{{/if}}\n        \"\n      >\n        <a class=\"account-nav-link\" href=\"{{urls.account.returns}}\">\n          {{lang 'core.account.menu.returns'}}\n        </a>\n      </li>\n    {{/if}}\n\n    <li\n      class=\"\n        account-nav-item\n        {{#if nav_page '===' 'messages'}}is-active{{/if}}\n      \"\n    >\n      <a class=\"account-nav-link\" href=\"{{urls.account.inbox}}\">\n        {{lang 'core.account.menu.messages'}}\n        {{#if customer.num_new_messages}}\n          <span class=\"account-nav-badge\">{{customer.num_new_messages}}</span>\n        {{/if}}\n      </a>\n    </li>\n\n    <li\n      class=\"\n        account-nav-item\n        {{#if nav_page '===' 'addresses'}}is-active{{/if}}\n      \"\n    >\n      <a class=\"account-nav-link\" href=\"{{urls.account.addresses}}\">\n        {{lang 'core.account.menu.addresses'}}\n      </a>\n    </li>\n\n    {{#if settings.show_wishlist}}\n      <li\n        class=\"\n          account-nav-item\n          {{#if nav_page '===' 'wishlists'}}is-active{{/if}}\n        \"\n      >\n        <a class=\"account-nav-link\" href=\"{{urls.account.wishlists.all}}\">\n          {{lang 'core.account.menu.wishlists'}}\n        </a>\n      </li>\n    {{/if}}\n\n    <li\n      class=\"\n        account-nav-item\n        {{#if nav_page '===' 'recent_items'}}is-active{{/if}}\n      \"\n    >\n      <a class=\"account-nav-link\" href=\"{{urls.account.recent_items}}\">\n        {{lang 'core.account.menu.recently_viewed'}}\n      </a>\n    </li>\n\n    <li\n      class=\"\n        account-nav-item\n        {{#if nav_page '===' 'settings'}}is-active{{/if}}\n      \"\n    >\n      <a class=\"account-nav-link\" href=\"{{urls.account.details}}\">\n        {{lang 'core.account.menu.settings'}}\n      </a>\n    </li>\n\n    {{#if settings.show_payment_methods}}\n      <li\n        class=\"\n          account-nav-item\n          {{#if nav_page '===' 'payment_methods'}}is-active{{/if}}\n        \"\n      >\n        <a class=\"account-nav-link\" href=\"{{urls.account.payment_methods.all}}\">\n          {{lang 'core.account.menu.payment_methods'}}\n        </a>\n      </li>\n    {{/if}}\n  </ul>\n\n  {{#if customer.store_credit.value '>' 0}}\n    {{> core/alerts/alert-success\n      (lang 'core.account.common.store_credit' store_credit=customer.store_credit.formatted)\n    }}\n  {{/if}}\n</nav>\n",
  "core/alerts/alert-success": "<div class=\"alert alert-success\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n"
}
