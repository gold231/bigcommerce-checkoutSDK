{
  "core/amp/common/share": "{{#if settings.add_this.buttons}}\n  <ul\n    class=\"social-links\"\n    data-addthis:url=\"{{url}}\"\n    data-addthis:title=\"{{title}}\"\n  >\n    {{#each settings.add_this.buttons}}\n      {{#all\n        (if service '!==' 'google')\n        (if service '!==' 'print')\n        (if service '!==' 'facebook')\n        (if service '!==' 'facebook_like')\n      }}\n        <li class=\"social-links-item social-links-item-{{service}}\">\n          {{#if service '===' 'google_plusone'}}\n            <amp-social-share\n              class=\"amp-social-share\"\n              data-params-product-link=\"{{url}}\"\n              data-params-product-id=\"{{id}}\"\n              data-params-product-name=\"{{name}}\"\n              data-params-social-type=\"gplus\"\n              type=\"gplus\"\n            ></amp-social-share>\n          {{else}}\n            <amp-social-share\n              class=\"amp-social-share\"\n              data-params-product-link=\"{{url}}\"\n              data-params-product-id=\"{{id}}\"\n              data-params-product-name=\"{{name}}\"\n              data-params-social-type=\"{{service}}\"\n              type=\"{{service}}\"\n            ></amp-social-share>\n          {{/if}}\n        </li>\n      {{/all}}\n    {{/each}}\n  </ul>\n{{/if}}\n"
}
